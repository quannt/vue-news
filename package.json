{
  "name": "vue-news",
  "version": "1.0.0",
  "description": "A news portal built powered by Vuejs and Nuxtjs",
  "author": "quannt",
  "private": true,
  "engines": {
    "node": "10.14.2",
    "npm": "6.4.1"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_PHASE=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production NUXT_PHASE=beta node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "npm run lint",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/proxy": "^1.3.1",
    "cross-env": "^5.2.0",
    "eslint-config-standard": "^12.0.0",
    "express": "^4.16.3",
    "newsapi": "^2.4.0",
    "nuxt": "^2.0.0",
    "timeago.js": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "tailwindcss": "~0.6.6"
  }
}
